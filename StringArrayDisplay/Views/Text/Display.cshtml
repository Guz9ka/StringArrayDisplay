@model List<string>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>StringsList</title>
</head>
<body>
    <form method="post">
        @{
            if (Model == null) return;

            foreach (var selectedString in Model)
            {
                <h1>@selectedString</h1>
            }
        }
    <</form>>
</body>
</html>
